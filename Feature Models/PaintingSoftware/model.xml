<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="autolayoutconstraints" value="false"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showconstraints" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="PaintingSoftware">
			<and mandatory="true" name="core">
				<or abstract="true" mandatory="true" name="OpenCanvas">
					<feature name="ExistingCanvas"/>
					<feature name="NewCanvas"/>
				</or>
				<and abstract="true" name="EditCanvas">
					<feature name="CopyAndPasteContent"/>
					<feature name="CutAndPasteContent"/>
					<and abstract="true" name="DrawOptions">
						<feature name="FillObject"/>
						<feature name="DrawShapes"/>
						<feature name="InsertText"/>
						<feature name="DrawFreeHand"/>
						<feature name="EraseContent"/>
					</and>
				</and>
				<and name="SaveCanvas">
					<feature name="SetFileName"/>
					<feature name="SetFileDirectory"/>
				</and>
			</and>
		</and>
	</struct>
</featureModel>
